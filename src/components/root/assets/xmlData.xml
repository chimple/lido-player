					<app-image visible="true" src="./build/assets/images/truck/truck.png" z="0" onEntry="this.marginTop='-230px';" width="1400px"></app-image>
					<app-image visible="true" src="./build/assets/images/truck/face.png" z="0" width="370px" x="650px" y="210px"></app-image>


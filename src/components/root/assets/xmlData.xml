<main>
	
	<app-container id="container" value="mainContainer1" objective="lion" ariaLabel="" height="100vh" width="100vw" x="0" y="0" z="0" bgColor="#FFFFFF" type="slide" visible="true" audio="background1.mp3" onMatch="" onTouch="" onCorrectTouch="this.border='2px solid green'; this.boxShadow='0 0 10px green';" onCorrectMatch="" onEntry="" onIncorrectTouch="this.border='2px solid red'; this.boxShadow='0 0 10px rgba(255, 0, 0, 0.5)';">

				<!-- Background -->
				<app-pos id="pos1" value="pos1" height="900px" width="1600px" ariaHidden="true" x="0" y="0" z="0" bgColor="transparent" type="" visible="true" audio="col1.mp3" onMatch="" onTouch="" onCorrectTouch="" onCorrectMatch="" onEntry="">
					<app-image visible="true" src="./build/assets/images/twoColorBg.jpg" z="0" width="1600px" height="900px" onEntry=""></app-image>
				</app-pos>

				<app-col visible="true" width="850px" height="500px" z="1" onEntry="this.animation='fallAndBounce 2s ease-in-out';">
					<app-text visible="true" width="600px" height="100px" onEntry="this.boxShadow='unset';" string='Which animal is known as the "king of the jungle"?' font="Poppins" font-size="25px" value="lion" bgColor="rgba(210, 140, 80, 1)"></app-text>

					<app-row visible="true" width="800px" height="100px" bgColor="transparent" >
						<app-col visible="true" width="150px" height="120px"  type="click" onEntry="" value="giraffe">
							<app-image visible="true" width="150px" height="100px" src="./build/assets/images/animals/giraffe.png" ></app-image>
							<app-text visible="true" width="150px" font="Poppins" font-size="20px" string="GIRAFFE" bgColor="transparent" onEntry="this.boxShadow='unset';"></app-text>
						</app-col>
						<app-col visible="true" width="150px" height="120px"  type="click" onEntry="" value="leopard">
							<app-image visible="true" width="150px" height="100px" src="./build/assets/images/animals/leopard.png" ></app-image>
							<app-text visible="true" width="150px" font="Poppins" font-size="20px" string="LEOPARD" bgColor="transparent" onEntry="this.boxShadow='unset';"></app-text>
						</app-col>
						<app-col visible="true" width="150px" height="120px"  type="click" onEntry="" value="lion">
							<app-image visible="true" width="150px" height="100px" src="./build/assets/images/animals/lion.png" ></app-image>
							<app-text visible="true" width="150px" font="Poppins" font-size="20px" string="LION" bgColor="transparent" onEntry="this.boxShadow='unset';"></app-text>
						</app-col>
						<app-col visible="true" width="150px" height="120px"  type="click" onEntry="" value="giraffe">
							<app-image visible="true" width="150px" height="100px" src="./build/assets/images/animals/tiger.png" ></app-image>
							<app-text visible="true" width="150px" font="Poppins" font-size="20px" string="TIGER" bgColor="transparent" onEntry="this.boxShadow='unset';"></app-text>
						</app-col>
						
						
					</app-row>
				</app-col>

	</app-container>

	<app-container id="container" value="mainContainer1" objective="I,can,see,with,my,eyes" ariaLabel="" height="100vh" width="100vw" x="0" y="0" z="0" bgColor="#FFFFFF" type="slide" visible="true" audio="background1.mp3" onMatch="" onTouch="" onCorrectTouch="" onCorrectMatch="truck.animation='placeToLeft 2s 1s linear';" onEntry="" onWrong="drag.backgroundColor='red';">

				<!-- Background -->
				<app-pos id="pos1" value="pos1" height="900px" width="1600px" ariaHidden="true" x="0" y="0" z="0" bgColor="transparent" type="" visible="true" audio="col1.mp3" onMatch="" onTouch="" onCorrectTouch="" onCorrectMatch="" onEntry="" >
					<app-image visible="true" src="./build/assets/images/truck/road.jpg" z="0" height="900px" width="1600px"></app-image>
				</app-pos>

				<!-- Drop Elements -->
				<app-pos id="truck" value="pos1" height="900px" width="1600px" ariaHidden="true" x="0" y="-160px" z="0" bgColor="transparent" type="" visible="true" audio="col1.mp3" onMatch="" onTouch="" onCorrectTouch="" onCorrectMatch="" onEntry="this.animation='rightToPlace 2s linear';">

					<app-image visible="true" src="./build/assets/images/truck/blueTruck.png" z="0" onEntry="this.marginTop='-100px';"></app-image>


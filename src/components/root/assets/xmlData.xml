					<app-text visible="true" tabIndex="2" onEntry="this.border='dashed 2px';" bgColor="white" z="0" width="100px" height="40px" x="900px" y="270px" type="drop" value="o2" onCorrectMatch="this.boxShadow='unset'; this.alignMatch='true';"></app-text>
					<app-text visible="true" tabIndex="3" onEntry="this.border='dashed 2px';" bgColor="white" z="0" width="100px" height="40px" x="650px" y="460px" type="drop" value="o3" onCorrectMatch="this.boxShadow='unset'; this.alignMatch='true';"></app-text>

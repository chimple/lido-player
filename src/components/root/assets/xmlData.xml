					</app-col>

					
					<app-row visible="true" bgColor="transparent" width="700px" height="150px" x="490px" y="620px">
						<app-text visible="true"  tabIndex="1" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="I" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
						<app-text visible="true" tabIndex="2" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="can" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
						<app-text visible="true" tabIndex="3" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="see" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
						<app-text visible="true" tabIndex="4" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="with" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
						<app-text visible="true" tabIndex="5" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="my" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
						<app-text visible="true" tabIndex="6" width="100px" height="80px" onEntry="this.border='dashed 2px';" bgColor="transparent" value="eyes" type="drop" onMatch="this.alignMatch='true';" onCorrectMatch="this.boxShadow='unset'; element.animation='unset';" onWrong=""></app-text>
					</app-row>
				</app-pos>

				

				<app-pos visible="true" bgColor="transparent" width="900px" height="150px" z="1" x="380px" y="700px">
					<app-row visible="true" bgColor="transparent" width="900px" height="150px" >
						<app-text visible="true" id="drag" tabIndex="7" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="see" value="see" type="drag" font="Poppins" font-size="25px"></app-text>
						<app-text visible="true" id="drag" tabIndex="8" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="I" value="I" type="drag" font="Poppins" font-size="25px"></app-text>
						<app-text visible="true" id="drag" tabIndex="9" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="eyes" value="eyes" type="drag" font="Poppins" font-size="25px"></app-text>
						<app-text visible="true" id="drag" tabIndex="10" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="my" value="my" type="drag" font="Poppins" font-size="25px"></app-text>
						<app-text visible="true" id="drag" tabIndex="11" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="with" value="with" type="drag" font="Poppins" font-size="25px"></app-text>
						<app-text visible="true" id="drag" tabIndex="12" width="100px" height="80px" onEntry="this.boxShadow='0 10px 0 0 white';" bgColor="#f7fc6d" string="can" value="can" type="drag" font="Poppins" font-size="25px"></app-text>
					</app-row>
				</app-pos>
					
				
				
	</app-container>

	<app-container id="container" value="mainContainer1" objective="o1,o2,o3,o4" ariaLabel="" height="100vh" width="100vw" x="0" y="0" z="0" bgColor="#FFFFFF" type="slide" visible="true" audio="background1.mp3" onMatch="" onTouch="" onCorrectTouch="" onCorrectMatch="truck.animation='placeToLeft 2s 1s linear';" onEntry="">

